# API Configuration
VITE_API_URL=https://prospertrack-api.onrender.com
# Comma-separated list of allowed CORS origins (leave empty for all)
CORS_ORIGIN=http://localhost:5173


# PostgreSQL Configuration
# You can use NETLIFY_DATABASE_URL or DATABASE_URL as a single connection string
# instead of the individual PG* settings below
NETLIFY_DATABASE_URL=
PGHOST=localhost
PGPORT=5432
PGUSER=postgres
PGPASSWORD=password
PGDATABASE=prospertrack

# Clerk Configuration
CLERK_PUBLISHABLE_KEY=your-clerk-publishable-key
VITE_CLERK_PUBLISHABLE_KEY=your-clerk-publishable-key
CLERK_SECRET_KEY=your-clerk-secret-key
